///////////CSS//////
inline > styleTag > stylesheet

inline
{
    background-color:red;
    font-size:30px
}

styleTag
{
    background-color:green;
    border:2px;
    color:orange
}

stylesheet{
    font-size:40px;
    border:3px;
    margin:10px
}

{
    background-color:red;
    font-size:30px;
    border:2px;
    color:orange;
    margin:10px
}


